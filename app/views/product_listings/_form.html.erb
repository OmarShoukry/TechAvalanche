<%= form_with model: product_listing do |form| %>
    <div>
        <%= form.label :"Product name" %><br>
        <%= form.collection_select :product_id, @products, :id, :name %>
        
        <br>
    </div>

    <div>
        <%= form.label :"Choose Warehouse" %><br>
        <%= form.collection_select :location_id, @locations, :id, :name %>
        
        <br>
    </div>

    <div>
        <%= form.label :Quantity %><br>
        <%= form.number_field :quantity %>
        <p id="error"><%= flash[:error] %></p>
        <br>
    </div>



    <div>
        <%= form.submit %>
    </div>

<% end %>

